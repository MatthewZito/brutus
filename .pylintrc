[MASTER]

ignore=__tests__
persistent=no
jobs=4
unsafe-load-any-extension=no

[FORMAT]

indent-string='    '
expected-line-ending-format=LF
no-space-check=empty-line

[BASIC]

good-names=cb,fn,n

[SIMILARITIES]

min-similarity-lines=300

[MESSAGES CONTROL]
disable=E0401,R1732,E0611,R0903,C0103,W0511,R0913,R0801,R0201
